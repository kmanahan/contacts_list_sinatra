<h1>Hello <%=@user.username%></h1>
<form action="/contacts/new">
<button type="submit">Add A Contact</button> 
</form>

<h2>Contacts List</h2> 

<div>
  <ul>
    <li><% @user.contacts.each do |attribute| %></li>
    <li><%=attribute.name%></li>
    <li><%=attribute.phone%></li>
    <li><%=attribute.email%></li>
    <li><%=attribute.address%></li>
  <%end%>
  </ul>
<form action="/contacts/:id/edit" method="post">
<button type="submit">Edit</button> 
</form>
<form action="/contacts/delete" method="post">
<button type="submit">Delete</button> 
<% #flash message are you sure you want to delete %>
</form>
</div>
